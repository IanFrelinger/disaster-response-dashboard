{
  "name": "disaster-response-dashboard",
  "version": "1.0.0",
  "description": "Geospatial Disaster Response Dashboard",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "dev:demo": "cp env.demo .env && vite",
    "dev:debug": "cp env.debug .env && vite",
    "dev:production": "cp env.production .env && vite",
    "build": "vite build",
    "build:demo": "cp env.demo .env && vite build",
    "build:debug": "cp env.debug .env && vite build",
    "build:production": "cp env.production .env && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:components": "node run-tests.js components",
    "test:hooks": "node run-tests.js hooks",
    "test:services": "node run-tests.js services",
    "test:utils": "node run-tests.js utils"
  },
  "dependencies": {
    "@osdk/client": "latest",
    "@osdk/react": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-map-gl": "^7.1.0",
    "mapbox-gl": "^3.0.0",
    "deck.gl": "^9.0.0",
    "@turf/turf": "^6.5.0",
    "h3-js": "^4.1.0",
    "@tanstack/react-query": "^5.0.0",
    "axios": "^1.6.0",
    "react-router-dom": "^6.8.0",
    "recharts": "^2.8.0",
    "lucide-react": "^0.294.0"
